    "@astrojs/rss": "^1.0.0",
    "@astrojs/sitemap": "^1.0.0",
    "@astrojs/tailwind": "^1.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.4",
    "astro": "^1.0.0",
    "astro-iconify": "^1.0.0",
    "limax": "^v2.1.0",
    "prettier": "^2.7.1",
    "reading-time": "^1.5.0",
    "subfont": "^6.12.2",
    "svgo": "^3.0.1"
  },
  "engines": {
    "node": ">=14"
