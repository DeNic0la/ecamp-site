{
  "name": "ecamp-site",
  "description": "Landingpage",
  "private": true,
  "scripts": {
    "dev": "astro dev --port=3004",
    "start": "astro dev",
    "build": "astro build && subfont -ir --no-fallbacks --silent --root dist",
