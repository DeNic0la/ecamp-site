  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strictNullChecks": true,
    "types": ["astro/client-image"],
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@/*": ["src/*"]
    },
  },
}
